{
  "name": "beauty-stars",
  "version": "1.1.0",
  "description": "A simple and beauty star rating for React projects",
  "author": "Leocardoso94",
  "license": "MIT",
  "repository": "Leocardoso94/beauty-stars",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "coverage": "yarn test --coverage && cat coverage/lcov.info | coveralls"
  },
  "dependencies": {},
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^5.0.0",
    "@testing-library/react": "^11.0.1",
    "@types/jest": "^26.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.7",
    "cross-env": "^7.0.0",
    "gh-pages": "^3.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts-ts": "^2.16.0",
    "react-test-renderer": "^17.0.0",
    "rollup": "^3.7.3",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^3.1.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-url": "^3.0.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "dist"
  ]
}
